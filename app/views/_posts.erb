<% if @posts.empty? %>
  <div class="col-12">
    <em>No results</em>
  </div>
<% else %>
  <% @posts.each { |post|
    json = JSON.parse(post['Iframely'])
    next unless json['links'] %>
  <div class="col-lg-4 mb-3">
    <%= partial :post, locals: { post: post, json: json } %>
  </div>
  <% } %>
<% end %>
